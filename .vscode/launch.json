            "mode": "debug",
            "program": "${workspaceRoot}\\cmd\\DnsCompare",
            "env": {},
            "cwd": "${workspaceRoot}\\cmd\\DnsCompare",
            "args": [
                "compare",
                "--Log.Level", "trace",
                "--ServerDef", "E:\\code\\DnsCompare\\servers_DEV.json",
